# Examples:
#
#   host *
#     IdentityFile /home/xxx/.ssh/id_ed25519
#
#   host 192.168.128.87
#     hostname 192.168.128.87
#     port 22
#     IdentityFile /home/xxx/.ssh/id_rsa_for_xxx
#     proxycommand ncat --proxy-type http --proxy $HPROXY_HOST:$HPROXY_PORT %h %p
#
#   host github.com
#     IdentityFile /home/xxx/.ssh/id_ed25519
#     proxycommand ncat --proxy-type http --proxy $HPROXY_HOST:$HPROXY_PORT %h %p
#
#
